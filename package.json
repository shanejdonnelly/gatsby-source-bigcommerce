{
    "name": "gatsby-source-bigcommerce-v2",
    "version": "0.4.3",
    "description": "Unofficial Gatsby plugin for BigCommerce API",
    "scripts": {
        "build": "babel src --out-dir .",
        "prepublishOnly": "npm run build",
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepare": "husky install"
    },
    "homepage": "https://github.com/guyromellemagayano/gatsby-source-bigcommerce-v2/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/guyromellemagayano/gatsby-source-bigcommerce-v2"
    },
    "keywords": [
        "gatsby",
        "gatsby-plugin",
        "gatsby-source-bigcommerce-v2",
        "plugin",
        "source",
        "big",
        "commerce",
        "bigcommerce",
        "api"
    ],
    "author": "Guy Romelle Magayano <aspiredtechie2010@gmail.com> (https://guyromellemagayano.dev)",
    "contributors": [
        {
            "name": "Grant Glidewell",
            "email": "grant@thirdandgrove.com"
        }
    ],
    "license": "MIT",
    "dependencies": {
        "babel-preset-gatsby-package": "^1.10.0",
        "http-proxy-middleware": "^2.0.1",
        "micro": "^9.3.4",
        "node-fetch": "^2.6.1"
    },
    "peerDependencies": {
        "gatsby": "^3.10.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.8",
        "@babel/core": "^7.14.8",
        "commitizen": "^4.2.4",
        "cross-env": "^7.0.3",
        "cz-conventional-changelog": "^3.3.0",
        "dotenv-cli": "^4.0.0",
        "eslint": "^7.31.0",
        "eslint-plugin-react": "^7.24.0",
        "husky": "^7.0.1",
        "lint-staged": "^11.1.1",
        "prettier": "^2.3.2"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
        }
    },
    "lint-staged": {
        "**/*.{js,jsx}": [
            "prettier --write",
            "git add"
        ],
        "**/*.{md,mdx,json}": [
            "prettier --write",
            "git add"
        ]
    }
}
